{
  "manifest_version": 2,

  "name": "yify torent",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "http://www.imdb.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
